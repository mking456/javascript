    const usedPorts = [50342];
    $("#terminal-proxy-configure-dialog").show();
    $(".proxy-configure-enter").prop("disabled", true);
    setTimeout(function () { $("#proxy-configure-input-entry").focus(); }, 100);
    $("#proxy-configure-input-entry").on('input', function () {
